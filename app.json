{
  "expo": {
    "name": "Jamiat Alh-e-Hadees",
    "slug": "prayertrackmobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon":"./assets/ios-quran-icon-light.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "buildNumber": "1",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to find nearby mosques for prayer times.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to your location to find nearby mosques for prayer times.",
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": [
          "remote-notification"
        ]
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.mubeen21.prayertrackmobile"
    },
    "android": {
      "versionCode": 1,
      "package": "com.mubeen21.prayertrackmobile",
      "permissions": [
        "ACCESS_FINE_LOCATION"
      ],
      "edgeToEdgeEnabled": true,
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-android-quran-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-font",
      [
        "expo-notifications",
        {
          "icon":"./assets/ios-quran-icon-light.png",
          "color": "#ffffff",
          "iosDisplayInForeground": true
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/ios-quran-icon-light.png",
          "imageWidth": 250
        }
      ]
    ],
    "scheme": "prayer-track-mobile",
    "owner": "mubeen21",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "262114a5-cdca-41c0-8c84-0f0e051fd358"
      }
    }
  }
}
